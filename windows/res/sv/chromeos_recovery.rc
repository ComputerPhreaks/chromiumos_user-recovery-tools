END

IDD_ERASE_CONFIRM DIALOGEX 0, 0, 237, 89
STYLE DS_SETFONT | DS_MODALFRAME | DS_FIXEDSYS | WS_POPUP | WS_CAPTION | WS_SYSMENU
FONT 8, "MS Shell Dlg", 400, 0, 0x1
BEGIN
    DEFPUSHBUTTON   "Ta bort",IDOK,126,68,50,14
    PUSHBUTTON      "Ta inte bort",IDCANCEL,180,68,50,14
    LTEXT           "Ta bort bekräftelsemeddelandet som anges vid körning",IDC_CONFIRM_MSG,38,7,192,29
    ICON            "",IDC_WARNING_ICON,7,7,21,20,SS_REALSIZEIMAGE
    LTEXT           "Alla befintliga data tas bort permanent. Det går inte att ångra den här åtgärden.",IDC_STATIC,38,42,192,17
END

IDD_PROPPAGE_FINISH DIALOGEX 0, 0, 380, 156
STYLE DS_SETFONT | DS_FIXEDSYS | WS_CHILD | WS_DISABLED | WS_CAPTION
FONT 8, "MS Shell Dlg", 400, 0, 0x0
BEGIN
    LTEXT           "Med det här verktyget kan du skapa en återställningskopia på ett SD-kort eller USB-minne som du kan använda för att återställa din Chromebook.",IDC_STATIC,7,7,366,20
